expr "$name" : '\(.*\)\.conf'

#find
* find symbols in object 
        find . -name 'a.out' -exec nm -D {} \; -print
        find . -name '*.o' -print0 | xargs -0 nm -A | egrep ' (i|y)$'
* rm
find . -name ‘your_pattern*’ -exec rm -f {} \;
find . -name ‘your_pattern*’ -delete
