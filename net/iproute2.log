
# ip link

## operstate
IFLA_OPERSTATE
oper_states and print_operstate

kernel: rtnl_fill_ifinfo
nla_put_u8(skb, IFLA_OPERSTATE, netif_running(dev) ? dev->operstate : IF_OPER_DOWN)
/* RFC 2863 operational status */
enum {
        IF_OPER_UNKNOWN,
        IF_OPER_NOTPRESENT,
        IF_OPER_DOWN,
        IF_OPER_LOWERLAYERDOWN,
        IF_OPER_TESTING,
        IF_OPER_DORMANT,
        IF_OPER_UP,
};

/sys/class/net/eth0/operstate
